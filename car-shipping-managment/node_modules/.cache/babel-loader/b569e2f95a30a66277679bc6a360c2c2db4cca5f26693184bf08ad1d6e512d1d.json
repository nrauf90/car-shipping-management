\n              <th class=\"px-4 py-3\">Action</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"car in cars\" :key=\"car.id\" class=\"border-b\">\r\n              <td class=\"px-4 py-3\">{{car?.id}}</td>\r\n              <td class=\"px-4 py-3\">{{car?.make}}</td>\r\n              <td class=\"px-4 py-3\">{{car?.model}}</td>\r\n              <td class=\"px-4 py-3\">{{car?.year}}</td>\r\n              <td class=\"px-4 py-3\">{{car?.vin}}</td>\r\n              <td class=\"px-4 py-3\">{{car?.shipping_status}}</td>\r\n              <td class=\"px-4 py-3\"><i class=\"fa fa-trash\"></i></td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n        <div class=\"pagination\">\r\n          <ul v-if=\"totalPages\">\r\n            <li v-for=\"page in totalPages\" :key=\"index\">page</li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      cars: [],\r\n      totalItem: 0,\r\n      perPage: 10,\r\n      totalPages: 0\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getCars()\r\n  },\r\n  methods: {\r\n    getCars(){\r\n      axios.get('cars', {\r\n        headers: {\r\n          'Accept': 'application/json',\r\n          'content-type': 'application/json',\r\n        }\r\n      }).then(response => {\r\n        this.cars = response.data.data;\r\n        this.totalItem = response.data.meta.total;\r\n        this.perPage = response.data.meta.per_page;\r\n        this.totalPages = Math.ceil(this.totalItem / this.perPage);\r\n\r\n      }).catch(error => {\r\n        console.log(error);\r\n      });\r\n    },\r\n    deleteCar(id) {\r\n      axios.delete(`cars/${id}`, {\r\n        headers: {\r\n          'Accept': 'application/json',\r\n          'content-type': 'application/json',\r\n        }\r\n      }).then(response => {\r\n        this.cars = this.cars.filter(car => car.id !== id);\r\n      }).catch(error => {\r\n        console.log(error);\r\n      });\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":"AAsCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,SAAS,EAAE,CAAC;MACZC,OAAO,EAAE,EAAE;MACXC,UAAU,EAAE;IACd;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,OAAO,CAAC;EACf,CAAC;EACDC,OAAO,EAAE;IACPD,OAAOA,CAAA,EAAE;MACPP,KAAK,CAACS,GAAG,CAAC,MAAM,EAAE;QAChBC,OAAO,EAAE;UACP,QAAQ,EAAE,kBAAkB;UAC5B,cAAc,EAAE;QAClB;MACF,CAAC,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;QAClB,IAAI,CAACV,IAAG,GAAIU,QAAQ,CAACX,IAAI,CAACA,IAAI;QAC9B,IAAI,CAACE,SAAQ,GAAIS,QAAQ,CAACX,IAAI,CAACY,IAAI,CAACC,KAAK;QACzC,IAAI,CAACV,OAAM,GAAIQ,QAAQ,CAACX,IAAI,CAACY,IAAI,CAACE,QAAQ;QAC1C,IAAI,CAACV,UAAS,GAAIW,IAAI,CAACC,IAAI,CAAC,IAAI,CAACd,SAAQ,GAAI,IAAI,CAACC,OAAO,CAAC;MAE5D,CAAC,CAAC,CAACc,KAAK,CAACC,KAAI,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACJ,CAAC;IACDG,SAASA,CAACC,EAAE,EAAE;MACZvB,KAAK,CAACwB,MAAM,CAAC,QAAQD,EAAE,EAAE,EAAE;QACzBb,OAAO,EAAE;UACP,QAAQ,EAAE,kBAAkB;UAC5B,cAAc,EAAE;QAClB;MACF,CAAC,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;QAClB,IAAI,CAACV,IAAG,GAAI,IAAI,CAACA,IAAI,CAACuB,MAAM,CAACC,GAAE,IAAKA,GAAG,CAACH,EAAC,KAAMA,EAAE,CAAC;MACpD,CAAC,CAAC,CAACL,KAAK,CAACC,KAAI,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACpB,CAAC,CAAC;IACJ;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}