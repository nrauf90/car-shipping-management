{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      cars: [],\n      totalItem: 0,\n      perPage: 10,\n      totalPages: 0,\n      make: '',\n      model: '',\n      year: '',\n      current: 1,\n      shipping_status: '',\n      addNew: false,\n      vin: ''\n    };\n  },\n  mounted() {\n    this.getCars({\n      page: 1\n    });\n  },\n  methods: {\n    getCars(params) {\n      axios.get('cars', {\n        headers: {\n          'Accept': 'application/json',\n          'content-type': 'application/json'\n        },\n        params\n      }).then(response => {\n        this.cars = response.data.data;\n        this.totalItem = response.data.meta.total;\n        this.perPage = response.data.meta.per_page;\n        this.current = response.data.meta.current_page;\n        this.totalPages = Math.ceil(this.totalItem / this.perPage);\n      });\n    },\n    nextPage(page) {\n      let params = {\n        page: page\n      };\n      if (this.make !== '') {\n        params.make = this.make;\n      }\n      if (this.model !== '') {\n        params.model = this.model;\n      }\n      if (this.year !== '') {\n        params.year = this.year;\n      }\n      if (this.shipping_status !== '') {\n        params.shipping_status = this.shipping_status;\n      }\n      this.getCars(params);\n    },\n    filterRecords() {\n      let params = {\n        page: 1\n      };\n      if (this.make !== '') {\n        params.make = this.make;\n      }\n      if (this.model !== '') {\n        params.model = this.model;\n      }\n      if (this.year !== '') {\n        params.year = this.year;\n      }\n      if (this.shipping_status !== '') {\n        params.shipping_status = this.shipping_status;\n      }\n      this.getCars(params);\n    },\n    addNewCar() {\n      let car = {\n        make: this.make,\n        model: this.model,\n        year: this.year,\n        vin: this.vin,\n        shipping_status: this.shipping_status\n      };\n      axios.post('cars', car, {\n        headers: {\n          'Accept': 'application/json',\n          'content-type': 'application/json'\n        }\n      }).then(() => {\n        //reset the form\n        this.make = '';\n        this.model = '';\n        this.year = '';\n        this.vin = '';\n        this.shipping_status = '';\n        this.showModal = false;\n        this.getCars({\n          page: 1\n        });\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    deleteRow(id) {\n      this.isChangeStatus = true;\n      axios.delete(`cars/${id}`, status, {\n        headers: {\n          'Accept': 'application/json',\n          'content-type': 'application/json',\n          'Authorization': `Bearer ${localStorage.getItem('token')}`\n        }\n      }).then(() => {\n        this.getCars({\n          page: 1\n        });\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    changeStatus(event, id) {\n      let status = {\n        shipping_status: event.target.value\n      };\n      console.log(status, id);\n    }\n  }\n};","map":{"version":3,"names":["axios","data","cars","totalItem","perPage","totalPages","make","model","year","current","shipping_status","addNew","vin","mounted","getCars","page","methods","params","get","headers","then","response","meta","total","per_page","current_page","Math","ceil","nextPage","filterRecords","addNewCar","car","post","showModal","catch","error","console","log","deleteRow","id","isChangeStatus","delete","status","localStorage","getItem","changeStatus","event","target","value"],"sources":["C:\\Personal\\car-shipping\\car-shipping-managment\\src\\pages\\CarsComponent.vue"],"sourcesContent":["<template>\r\n  <div class=\"row\">\r\n    <div class=\"col-md-12\">\r\n      <h2>Cars</h2>\r\n      <button class=\"btn btn-primary\" @click.prevent=\"addNew = true\">Add New Car</button>\r\n      <div class=\"row\" v-if=\"addNew\">\r\n        <div class=\"col-md-6 offset-md-3\">\r\n          <div class=\"card\">\r\n            <form @submit.prevent=\"addNewCar\">\r\n              <div class=\"card-header\">\r\n                <h3>Add New Car</h3>\r\n              </div>\r\n              <div class=\"card-body\">\r\n                  <div class=\"form-group\">\r\n                    <label for=\"make\">Make</label>\r\n                    <input type=\"text\" v-model.trim=\"make\" class=\"form-control\" id=\"make\" placeholder=\"Make\">\r\n                  </div>\r\n                  <div class=\"form-group\">\r\n                    <label for=\"model\">Model</label>\r\n                    <input type=\"text\" v-model.trim=\"model\" class=\"form-control\" id=\"model\" placeholder=\"Model\">\r\n                  </div>\r\n                  <div class=\"form-group\">\r\n                    <label for=\"year\">Year</label>\r\n                    <input type=\"text\" v-model.trim=\"year\" class=\"form-control\" id=\"year\" placeholder=\"Year\">\r\n                  </div>\r\n                  <div class=\"form-group\">\r\n                    <label for=\"vin\">VIN</label>\r\n                    <input type=\"text\" v-model.trim=\"vin\" class=\"form-control\" id=\"vin\" placeholder=\"VIN\">\r\n                  </div>\r\n                  <div class=\"form-group\">\r\n                    <label for=\"shipping_status\">Shipping Status</label>\r\n                    <input type=\"text\" v-model.trim=\"shipping_status\" class=\"form-control\" id=\"shipping_status\"\r\n                           placeholder=\"Shipping Status\">\r\n                  </div>\r\n              </div>\r\n              <div class=\"card-footer text-center\">\r\n                <button type=\"submit\" class=\"btn btn-primary mx-3\">Add Car</button>\r\n                <button class=\"btn btn-danger\" @click.prevent=\"addNew = false\">Cancel</button>\r\n              </div>\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div v-if=\"!addNew\">\r\n        <form @submit.prevent=\"filterRecords\">\r\n          <div class=\"row\">\r\n            <div class=\"col-md-3\">\r\n              <div class=\"form-group\">\r\n                <label for=\"make\">Make</label>\r\n                <input type=\"text\" v-model.trim=\"make\" class=\"form-control\" id=\"make\" placeholder=\"Make\">\r\n              </div>\r\n            </div>\r\n            <div class=\"col-md-2\">\r\n              <div class=\"form-group\">\r\n                <label for=\"model\">Model</label>\r\n                <input type=\"text\" v-model.trim=\"model\" class=\"form-control\" id=\"model\" placeholder=\"Model\">\r\n              </div>\r\n            </div>\r\n            <div class=\"col-md-2\">\r\n              <div class=\"form-group\">\r\n                <label for=\"year\">Year</label>\r\n                <input type=\"text\" v-model.trim=\"year\" class=\"form-control\" id=\"year\" placeholder=\"Year\">\r\n              </div>\r\n            </div>\r\n            <div class=\"col-md-3\">\r\n              <div class=\"form-group\">\r\n                <label for=\"shipping_status\">Shipping Status</label>\r\n                <input type=\"text\" v-model.trim=\"shipping_status\" class=\"form-control\" id=\"shipping_status\"\r\n                       placeholder=\"Shipping Status\">\r\n              </div>\r\n            </div>\r\n            <div class=\"col-md-2\">\r\n              <div class=\"form-group\">\r\n                <label class=\"invisible\" for=\"shipping_status\">Filter</label>\r\n                <button type=\"submit\" class=\"form-control btn btn-primary\">Filter</button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n        </form>\r\n        <table class=\"table table-striped\">\r\n          <thead class=\"text-xs text-white text-uppercase bg-dark\">\r\n          <tr>\r\n            <th class=\"px-4 py-3\">ID</th>\r\n            <th class=\"px-4 py-3\">Make</th>\r\n            <th class=\"px-4 py-3\">Model</th>\r\n            <th class=\"px-4 py-3\">Year</th>\r\n            <th class=\"px-4 py-3\">VIN</th>\r\n            <th class=\"px-4 py-3\">Shipping Status</th>\r\n            <th class=\"px-4 py-3\">Action</th>\r\n          </tr>\r\n          </thead>\r\n          <tbody>\r\n          <tr v-for=\"car in cars\" :key=\"car.id\" class=\"border-b\">\r\n            <td class=\"px-4 py-3\">{{ car.id }}</td>\r\n            <td class=\"px-4 py-3\">{{ car.make }}</td>\r\n            <td class=\"px-4 py-3\">{{ car.model }}</td>\r\n            <td class=\"px-4 py-3\">{{ car.year }}</td>\r\n            <td class=\"px-4 py-3\">{{ car.vin }}</td>\r\n            <td class=\"px-4 py-3\">{{ car.shipping_status }}</td>\r\n            <td class=\"px-4 py-3\">\r\n              <a href=\"#\" class=\"my-1\" @click.prevent=\"deleteRow(car.id)\">Delete</a>\r\n              <select id=\"\" class=\"form-select\" @change.prevent=\"changeStatus($event, car.id)\">\r\n                <option value=\"\">Change Status</option>\r\n                <option value=\"shipped\">Shipped</option>\r\n                <option value=\"pending\">Pending</option>\r\n                <option value=\"processing\">Processing</option>\r\n              </select>\r\n            </td>\r\n          </tr>\r\n          </tbody>\r\n        </table>\r\n        <div class=\"pagination text-center\">\r\n          <ul class=\"list-inline mx-auto\" v-if=\"totalPages\">\r\n            <li class=\"list-inline-item px-1\" v-if=\"current !=1\">\r\n              <a href=\"#\" class=\"text-decoration-none\"\r\n                 @click.prevent=\"nextPage(1)\">First</a></li>\r\n            <li class=\"list-inline-item px-1\" v-for=\"index in totalPages\" :key=\"index\">\r\n              <a href=\"#\"\r\n                 class=\"text-decoration-none\"\r\n                 @click.prevent=\"nextPage(index)\">{{\r\n                  index\r\n                }}</a></li>\r\n            <li class=\"list-inline-item px-1\" v-if=\"totalPages>1 && current != totalPages\">\r\n              <a href=\"#\"\r\n                 class=\"text-decoration-none\"\r\n                 @click.prevent=\"nextPage(totalPages)\">Last</a>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      cars: [],\r\n      totalItem: 0,\r\n      perPage: 10,\r\n      totalPages: 0,\r\n      make: '',\r\n      model: '',\r\n      year: '',\r\n      current: 1,\r\n      shipping_status: '',\r\n      addNew: false,\r\n      vin: '',\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getCars({\r\n      page: 1\r\n    })\r\n  },\r\n  methods: {\r\n    getCars(params) {\r\n      axios.get('cars', {\r\n        headers: {\r\n          'Accept': 'application/json',\r\n          'content-type': 'application/json',\r\n        },\r\n        params\r\n      }).then(response => {\r\n        this.cars = response.data.data;\r\n        this.totalItem = response.data.meta.total;\r\n        this.perPage = response.data.meta.per_page;\r\n        this.current = response.data.meta.current_page;\r\n        this.totalPages = Math.ceil(this.totalItem / this.perPage);\r\n\r\n      });\r\n    },\r\n    nextPage(page) {\r\n      let params = {\r\n        page: page\r\n      }\r\n      if (this.make !== '') {\r\n        params.make = this.make;\r\n      }\r\n      if (this.model !== '') {\r\n        params.model = this.model;\r\n      }\r\n      if (this.year !== '') {\r\n        params.year = this.year;\r\n      }\r\n      if (this.shipping_status !== '') {\r\n        params.shipping_status = this.shipping_status;\r\n      }\r\n\r\n      this.getCars(params);\r\n    },\r\n    filterRecords() {\r\n      let params = {\r\n        page: 1\r\n      }\r\n      if (this.make !== '') {\r\n        params.make = this.make;\r\n      }\r\n      if (this.model !== '') {\r\n        params.model = this.model;\r\n      }\r\n      if (this.year !== '') {\r\n        params.year = this.year;\r\n      }\r\n      if (this.shipping_status !== '') {\r\n        params.shipping_status = this.shipping_status;\r\n      }\r\n      this.getCars(params)\r\n    },\r\n    addNewCar() {\r\n      let car = {\r\n        make: this.make,\r\n        model: this.model,\r\n        year: this.year,\r\n        vin: this.vin,\r\n        shipping_status: this.shipping_status\r\n      }\r\n      axios.post('cars', car, {\r\n        headers: {\r\n          'Accept': 'application/json',\r\n          'content-type': 'application/json',\r\n        }\r\n      }).then(() => {\r\n        //reset the form\r\n        this.make = '';\r\n        this.model = '';\r\n        this.year = '';\r\n        this.vin = '';\r\n        this.shipping_status = '';\r\n\r\n        this.showModal = false;\r\n        this.getCars({\r\n          page: 1\r\n        })\r\n      }).catch(error => {\r\n        console.log(error)\r\n      })\r\n    },\r\n    deleteRow(id){\r\n      this.isChangeStatus = true;\r\n\r\n      axios.delete(`cars/${id}`, status, {\r\n        headers: {\r\n          'Accept': 'application/json',\r\n          'content-type': 'application/json',\r\n          'Authorization': `Bearer ${localStorage.getItem('token')}`\r\n        }\r\n      }).then(() => {\r\n        this.getCars({\r\n          page: 1\r\n        })\r\n      }).catch(error => {\r\n        console.log(error)\r\n      });\r\n    },\r\n    changeStatus(event, id){\r\n      let status = {\r\n        shipping_status: event.target.value\r\n      }\r\n      console.log(status, id)\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":"AAwIA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,SAAS,EAAE,CAAC;MACZC,OAAO,EAAE,EAAE;MACXC,UAAU,EAAE,CAAC;MACbC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,CAAC;MACVC,eAAe,EAAE,EAAE;MACnBC,MAAM,EAAE,KAAK;MACbC,GAAG,EAAE;IACP;EACF,CAAC;EACDC,OAAOA,CAAA,EAA