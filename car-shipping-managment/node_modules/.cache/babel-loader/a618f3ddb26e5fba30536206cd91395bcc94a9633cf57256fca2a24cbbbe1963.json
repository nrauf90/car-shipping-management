{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  data() {\n    return {\n      cars: [],\n      totalItem: 0,\n      perPage: 10,\n      totalPages: 0,\n      make: '',\n      model: '',\n      year: '',\n      current: 1,\n      shipping_status: ''\n    };\n  },\n  mounted() {\n    this.getCars({\n      page: 1\n    });\n  },\n  methods: {\n    getCars(params) {\n      axios.get('cars', {\n        headers: {\n          'Accept': 'application/json',\n          'content-type': 'application/json'\n        },\n        params\n      }).then(response => {\n        this.cars = response.data.data;\n        this.totalItem = response.data.meta.total;\n        this.perPage = response.data.meta.per_page;\n        this.current = response.data.meta.current_page;\n        this.totalPages = Math.ceil(this.totalItem / this.perPage);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    nextPage(page) {\n      let params = {\n        page: page\n      };\n      if (this.make !== '') {\n        params.make = this.make;\n      }\n      if (this.model !== '') {\n        params.model = this.model;\n      }\n      if (this.year !== '') {\n        params.year = this.year;\n      }\n      if (this.shipping_status !== '') {\n        params.shipping_status = this.shipping_status;\n      }\n      this.getCars(params);\n    },\n    filterRecords() {\n      let params = {\n        page: 1\n      };\n      if (this.make !== '') {\n        params.make = this.make;\n      }\n      if (this.model !== '') {\n        params.model = this.model;\n      }\n      if (this.year !== '') {\n        params.year = this.year;\n      }\n      if (this.shipping_status !== '') {\n        params.shipping_status = this.shipping_status;\n      }\n      this.getCars(params);\n    }\n  }\n};","map":{"version":3,"names":["axios","data","cars","totalItem","perPage","totalPages","make","model","year","current","shipping_status","mounted","getCars","page","methods","params","