\r\n            </div>\r\n          </div>\r\n\r\n        </form>\r\n        <table class=\"table table-striped\">\r\n          <thead class=\"text-xs text-white text-uppercase bg-dark\">\r\n            <tr>\r\n              <th class=\"px-4 py-3\">ID</th>\r\n              <th class=\"px-4 py-3\">Make</th>\r\n              <th class=\"px-4 py-3\">Model</th>\r\n              <th class=\"px-4 py-3\">Year</th>\r\n              <th class=\"px-4 py-3\">VIN</th>\r\n              <th class=\"px-4 py-3\">Shipping Status</th>\r\n              <th class=\"px-4 py-3\">Action</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"car in cars\" :key=\"car.id\" class=\"border-b\">\r\n              <td class=\"px-4 py-3\">{{car?.id}}</td>\r\n              <td class=\"px-4 py-3\">{{car?.make}}</td>\r\n              <td class=\"px-4 py-3\">{{car?.model}}</td>\r\n              <td class=\"px-4 py-3\">{{car?.year}}</td>\r\n              <td class=\"px-4 py-3\">{{car?.vin}}</td>\r\n              <td class=\"px-4 py-3\">{{car?.shipping_status}}</td>\r\n              <td class=\"px-4 py-3\"><i class=\"fa fa-trash\"></i></td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n        <div class=\"pagination text-center\">\r\n          <ul class=\"list-inline mx-auto\" v-if=\"totalPages\">\r\n              <li class=\"list-inline-item px-1\" v-if=\"current !=1\"><a href=\"#\" class=\"text-decoration-none\" @click.prevent=\"nextPage(1)\">First</a></li>\r\n              <li class=\"list-inline-item px-1\" v-for=\"index in totalPages\" :key=\"index\"><a href=\"#\" class=\"text-decoration-none\" @click.prevent=\"nextPage(index)\">{{ index }}</a></li>\r\n            <li class=\"list-inline-item px-1\" v-if=\"totalPages>1 && current != totalPages\"><a href=\"#\" class=\"text-decoration-none\" @click.prevent=\"nextPage(totalPages)\">Last</a></li>\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      cars: [],\r\n      totalItem: 0,\r\n      perPage: 10,\r\n      totalPages: 0,\r\n      make: '',\r\n      model:'',\r\n      year:'',\r\n      current:1,\r\n      shipping_status:'',\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getCars({\r\n        page: 1\r\n    })\r\n  },\r\n  methods: {\r\n    getCars(params){\r\n      axios.get('cars', {\r\n        headers: {\r\n          'Accept': 'application/json',\r\n          'content-type': 'application/json',\r\n        },\r\n        params\r\n      }).then(response => {\r\n        this.cars = response.data.data;\r\n        this.totalItem = response.data.meta.total;\r\n        this.perPage = response.data.meta.per_page;\r\n        this.current = response.data.meta.current_page;\r\n        this.totalPages = Math.ceil(this.totalItem / this.perPage);\r\n\r\n      }).catch(error => {\r\n        console.log(error);\r\n      });\r\n    },\r\n    nextPage(page){\r\n      let params = {\r\n        page: page\r\n      }\r\n      if(this.make !== ''){\r\n        params.make = this.make;\r\n      }\r\n      if(this.model !== ''){\r\n        params.model = this.model;\r\n      }\r\n      if(this.year !== ''){\r\n        params.year = this.year;\r\n      }\r\n      if(this.shipping_status !== ''){\r\n        params.shipping_status = this.shipping_status;\r\n      }\r\n\r\n      this.getCars(params);\r\n    },\r\n    filterRecords(){\r\n      let params = {\r\n        page: 1\r\n      }\r\n      if(this.make !== ''){\r\n        params.make = this.make;\r\n      }\r\n      if(this.model !== ''){\r\n        params.model = this.model;\r\n      }\r\n      if(this.year !== ''){\r\n        params.year = this.year;\r\n      }\r\n      if(this.shipping_status !== ''){\r\n        params.shipping_status = this.shipping_status;\r\n      }\r\n      this.getCars(params)\r\n    }\r\n  }\r\n}\r\n</script>"],"mappings":"AA2EA,OAAOA,KAAI,MAAO,OAAO;AAEzB,e